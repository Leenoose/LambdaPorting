{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Config": {
    "SSMParameterStorePath": "/cmw/test"
  },
  "Lambda.Logging": {
    "IncludeCategory": false,
    "IncludeLogLevel": false,
    "IncludeNewline": true,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information"
    }
  },
  "AWS": {
    "Region": "ap-southeast-1"
  },
  "NewSkiesApiSettings": {
    "BaseUrl": "https://tr-dotrezapi.test.tr.navitaire.com"
  },
  //Start of Scoot.Library.Core.Web Settings
  "JwtAuthStrategyConfig": {
    "TargetClaim": "tkn",
    "Issuer": "scoot-cmw", //Rename this to match your layer (CMW, AMW)
    "Audience": "scoot-app",
    "SkipExpiry": true,
    "SecretPath": "shared:jwtmiddlewaresecret", //Path to configuration
    "Algorithm": "http://www.w3.org/2001/04/xmldsig-more#hmac-sha256"
  },
  "AuthHandlerConfig": {
    "ExternalErrorTypeKeys": [ "Authentication", "Expired", "Credential", "Token" ]
  },
  "AppClientHeaderKeysConfig": {
    "NameKey": "X-Scoot-AppSource",
    "VersionKey": "X-Scoot-Client-Version",
    "EnvironmentKey": "X-Scoot-Client-Environment",
    "SessionKey": "X-Scoot-Client-Session"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Properties": {
      "Application": "otel-playgound" //Rename to match your application name (not project name)
    }
  }
}